<div class="row">
    {{#each articles}}
    <div class="col-md-3">
        <div class="card">
            <img src"..." class="card-img-top" alt="...">
            <div class="card-body">
                <h3 class="card-title">{{this.title}}</h3>
                <p class="card-text">{{this.summary}}</p>
                <a href="{{this.url}}">link</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>